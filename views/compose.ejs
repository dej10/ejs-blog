<%- include("partials/header"); -%>


<h1>Compose</h1>
<form action="/compose" method="post" autocomplete="off" >
	<div class="form-group">
		<label>Title</label>
		<input class="form-control" cols="150" type="text" name="blogTitle" placeholder="">
	</div>
	<div class="form-group">
		<label for="posts">Post</label>
		<textarea class="form-control" name="blogPost" rows="4" cols="150"> </textarea>
	</div>
	<button class=" btn btn-primary" type="submit">Publish</button>
</form>

<%- include("partials/footer"); -%>